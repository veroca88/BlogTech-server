<div class="container-fluid">

    <div class="row">
        {{#each users}}
        <div class="square col col-sm">
            <div class="card">
                <img src="https://avatars.dicebear.com/api/{{gender}}/{{username}}.svg" class="img-fluid  card-img-top"
                    alt="avatar">
                <div class="card-body">
                    <p class="card-text">{{username}}</p>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>